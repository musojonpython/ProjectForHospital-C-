  Form_Shifokor->ADOQuery1->SQL->Add("select " + satr + " from Bemorlar_haqida");
  Form_Shifokor->ADOQuery1->Open();
  Form_Shifokor->ADOQuery1->Active = true;
  ShowMessage(Form_Shifokor->ADOQuery1->SQL->Text);

  }
//---------------------------------------------------------------------------

void __fastcall TForm_Malumotqoshish::SpeedButton1MouseLeave(TObject *Sender)
{
   SpeedButton1->Font->Size = 12;
   SpeedButton1->Font->Color = clBlack;
}
//---------------------------------------------------------------------------

void __fastcall TForm_Malumotqoshish::SpeedButton1MouseMove(TObject *Sender, TShiftState Shift,
		  int X, int Y)
{
   SpeedButton1->Font->Size = 16;
   SpeedButton1->Font->Color = clRed;
}
//---------------------------------------------------------------------------

void __fastcall TForm_Malumotqoshish::SpeedButton2MouseLeave(TObject *Sender)
{
  SpeedButton2->Font->Size = 12;
   SpeedButton2->Font->Color = clBlack;
}
//---------------------------------------------------------------------------

